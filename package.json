{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "Kollegorna's JavaScript utilities",
  "main": "",
  "scripts": {
    "dev": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kollegorna/js-utils.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kollegorna/js-utils/issues"
  },
  "homepage": "https://github.com/kollegorna/js-utils#readme",
  "devDependencies": {
    "child_process": "^1.0.2",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.2",
    "path": "^0.12.7"
  },
  "dependencies": {},
  "eslintConfig": {
    "root": true,
    "extends": "eslint:recommended",
    "env": {
      "node": true,
      "es6": true,
      "amd": true,
      "browser": true
    },
    "parserOptions": {
      "ecmaFeatures": {
        "globalReturn": true,
        "generators": false,
        "objectLiteralDuplicateProperties": false,
        "experimentalObjectRestSpread": true
      },
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "plugins": [
      "import"
    ],
    "settings": {
      "import/core-modules": [],
      "import/ignore": [
        "node_modules",
        "\\.(coffee|scss|css|less|hbs|svg|json)$"
      ]
    },
    "rules": {
      "no-console": 0,
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "ignore"
        }
      ]
    }
  }
}
